{
  "name": "skeleton-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@gitlab.com:codingsans/almateam/skeleton-backend.git",
  "license": "MIT",
  "private": true,
  "description": "skeleton API",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "build": "tsc && yarn copy-sql-files",
    "test": "jest",
    "start": "node ./build/src/index.js",
    "copy-sql-files": "cp -R ./src/database/migrations/sql ./build/src/database/migrations",
    "migrate:dev": "ts-node --require dotenv/config ./src/database/migrate.ts",
    "migrate": "node ./build/src/database/migrate.js",
    "start:dev": "ts-node-dev --ignore-watch node_modules --require dotenv/config src/index.ts"
  },
  "devDependencies": {
    "@codingsans/eslint-config": "3.1.0",
    "@types/bluebird": "3.5.29",
    "@types/cls-hooked": "4.3.0",
    "@types/convict": "4.2.1",
    "@types/jest": "24.0.23",
    "@types/json-schema": "7.0.3",
    "@types/node": "12.12.18",
    "@types/pg": "7.11.2",
    "@types/pino": "5.14.0",
    "@types/ramda": "0.26.38",
    "@types/uuid": "3.4.6",
    "dotenv": "8.2.0",
    "eslint": "6.7.2",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.7.3"
  },
  "dependencies": {
    "bluebird": "3.7.2",
    "cls-hooked": "4.2.2",
    "convict": "5.2.0",
    "fastify": "2.11.0",
    "fastify-oas": "2.5.0",
    "knex": "0.20.4",
    "pg": "7.14.0",
    "pg-connection-string": "2.1.0",
    "pg-database-url": "0.1.0",
    "pino": "5.15.0",
    "ramda": "0.26.1",
    "uuid": "3.3.3"
  }
}
